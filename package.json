{
  "name": "n8n-nodes-wordpress-enhanced",
  "version": "0.1.0",
  "description": "Enhanced WordPress node for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Yan",
    "email": "yamcs@me.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yanmcs/n8n-improved-wordpress-node/"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts package.json",
    "lintfix": "eslint src/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/WordpressApiCredentials.credentials.js"
    ],
    "nodes": [
      "dist/WordpressEnhanced.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
